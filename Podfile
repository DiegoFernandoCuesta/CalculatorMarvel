  pod 'lottie-ios'
  pod 'SwiftyJSON'

  target 'CalculatorTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'CalculatorUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
